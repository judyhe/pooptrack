  <%= stylesheet_link_tag :all %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="header_wrapper">
	<div id="header" class="clearfix">
		<%= link_to "Pooptrack Home", root_path, :id => "logo" %>
